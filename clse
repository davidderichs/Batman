[31m-[m
[31m-  this.recalculateCoefficients = function() {[m
[31m-    var A;[m
[31m-    if (type === DSP.PEAKING_EQ || type === DSP.LOW_SHELF || type === DSP.HIGH_SHELF ) {[m
[31m-      A = Math.pow(10, (this.dBgain/40));  // for peaking and shelving EQ filters only[m
[31m-    } else {[m
[31m-      A  = Math.sqrt( Math.pow(10, (this.dBgain/20)) );[m
[31m-    }[m
[31m-[m
[31m-    var w0 = DSP.TWO_PI * this.f0 / this.Fs;[m
[31m-[m
[31m-    var cosw0 = Math.cos(w0);[m
[31m-    var sinw0 = Math.sin(w0);[m
[31m-[m
[31m-    var alpha = 0;[m
[31m-[m
[31m-    switch (this.parameterType) {[m
[31m-      case DSP.Q:[m
[31m-        alpha = sinw0/(2*this.Q);[m
[31m-        break;[m
[31m-[m
[31m-      case DSP.BW:[m
[31m-        alpha = sinw0 * sinh( Math.LN2/2 * this.BW * w0/sinw0 );[m
[31m-        break;[m
[31m-[m
[31m-      case DSP.S:[m
[31m-        alpha = sinw0/2 * Math.sqrt( (A + 1/A)*(1/this.S - 1) + 2 );[m
[31m-        break;[m
[31m-    }[m
[31m-[m
[31m-    /**[m
[31m-        FYI: The relationship between bandwidth and Q is[m
[31m-             1/Q = 2*sinh(ln(2)/2*BW*w0/sin(w0))     (digital filter w BLT)[m
[31m-        or   1/Q = 2*sinh(ln(2)/2*BW)             (analog filter prototype)[m
[31m-[m
[31m-        The relationship between shelf slope and Q is[m
[31m-             1/Q = sqrt((A + 1/A)*(1/S - 1) + 2)[m
[31m-    */[m
[31m-[m
[31m-    var coeff;[m
[31m-[m
[31m-    switch (this.type) {[m
[31m-      case DSP.LPF:       // H(s) = 1 / (s^2 + s/Q + 1)[m
[31m-        this.b0 =  (1 - cosw0)/2;[m
[31m-        this.b1 =   1 - cosw0;[m
[31m-        this.b2 =  (1 - cosw0)/2;[m
[31m-        this.a0 =   1 + alpha;[m
[31m-        this.a1 =  -2 * cosw0;[m
[31m-        this.a2 =   1 - alpha;[m
[31m-        break;[m
[31m-[m
[31m-      case DSP.HPF:       // H(s) = s^2 / (s^2 + s/Q + 1)[m
[31m-        this.b0 =  (1 + cosw0)/2;[m
[31m-        this.b1 = -(1 + cosw0);[m
[31m-        this.b2 =  (1 + cosw0)/2;[m
[31m-        this.a0 =   1 + alpha;[m
[31m-        this.a1 =  -2 * cosw0;[m
[31m-        this.a2 =   1 - alpha;[m
[31m-        break;[m
[31m-[m
[31m-      case DSP.BPF_CONSTANT_SKIRT:       // H(s) = s / (s^2 + s/Q + 1)  (constant skirt gain, peak gain = Q)[m
[31m-        this.b0 =   sinw0/2;[m
[31m-        this.b1 =   0;[m
[31m-        this.b2 =  -sinw0/2;[m
[31m-        this.a0 =   1 + alpha;[m
[31m-        this.a1 =  -2*cosw0;[m
[31m-        this.a2 =   1 - alpha;[m
[31m-        break;[m
[31m-[m
[31m-      case DSP.BPF_CONSTANT_PEAK:       // H(s) = (s/Q) / (s^2 + s/Q + 1)      (constant 0 dB peak gain)[m
[31m-        this.b0 =   alpha;[m
[31m-        this.b1 =   0;[m
[31m-        this.b2 =  -alpha;[m
[31m-        this.a0 =   1 + alpha;[m
[31m-        this.a1 =  -2*cosw0;[m
[31m-        this.a2 =   1 - alpha;[m
[31m-        break;[m
[31m-[m
[31m-      case DSP.NOTCH:     // H(s) = (s^2 + 1) / (s^2 + s/Q + 1)[m
[31m-        this.b0 =   1;[m
[31m-        this.b1 =  -2*cosw0;[m
[31m-        this.b2 =   1;[m
[31m-        this.a0 =   1 + alpha;[m
[31m-        this.a1 =  -2*cosw0;[m
[31m-        this.a2 =   1 - alpha;[m
[31m-        break;[m
[31m-[m
[31m-      case DSP.APF:       // H(s) = (s^2 - s/Q + 1) / (s^2 + s/Q + 1)[m
[31m-        this.b0 =   1 - alpha;[m
[31m-        this.b1 =  -2*cosw0;[m
[31m-        this.b2 =   1 + alpha;[m
[31m-        this.a0 =   1 + alpha;[m
[31m-        this.a1 =  -2*cosw0;[m
[31m-        this.a2 =   1 - alpha;[m
[31m-        break;[m
[31m-[m
[31m-      case DSP.PEAKING_EQ:  // H(s) = (s^2 + s*(A/Q) + 1) / (s^2 + s/(A*Q) + 1)[m
[31m-        this.b0 =   1 + alpha*A;[m
[31m-        this.b1 =  -2*cosw0;[m
[31m-        this.b2 =   1 - alpha*A;[m
[31m-        this.a0 =   1 + alpha/A;[m
[31m-        this.a1 =  -2*cosw0;[m
[31m-        this.a2 =   1 - alpha/A;[m
[31m-        break;[m
[31m-[m
[31m-      case DSP.LOW_SHELF:   // H(s) = A * (s^2 + (sqrt(A)/Q)*s + A)/(A*s^2 + (sqrt(A)/Q)*s + 1)[m
[31m-        coeff = sinw0 * Math.sqrt( (A^2 + 1)*(1/this.S - 1) + 2*A );[m
[31m-        this.b0 =    A*((A+1) - (A-1)*cosw0 + coeff);[m
[31m-        this.b1 =  2*A*((A-1) - (A+1)*cosw0);[m
[31m-        this.b2 =    A*((A+1) - (A-1)*cosw0 - coeff);[m
[31m-        this.a0 =       (A+1) + (A-1)*cosw0 + coeff;[m
[31m-        this.a1 =   -2*((A-1) + (A+1)*cosw0);[m
[31m-        this.a2 =       (A+1) + (A-1)*cosw0 - coeff;[m
[31m-        break;[m
