[31m-    this.Q = Math.max(Math.min(q, 115.0), 0.001);[m
[31m-    this.recalculateCoefficients();[m
[31m-  };[m
[31m-[m
[31m-  this.setBW = function(bw) {[m
[31m-    this.parameterType = DSP.BW;[m
[31m-    this.BW = bw;[m
[31m-    this.recalculateCoefficients();[m
[31m-  };[m
[31m-[m
