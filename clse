[31m-  this.x_2_r = 0;[m
[31m-  this.y_1_r = 0;[m
[31m-  this.y_2_r = 0;[m
[31m-[m
[31m-  this.b0 = 1;[m
[31m-  this.a0 = 1;[m
[31m-[m
[31m-  this.b1 = 0;[m
[31m-  this.a1 = 0;[m
[31m-[m
[31m-  this.b2 = 0;[m
[31m-  this.a2 = 0;[m
[31m-[m
[31m-  this.b0a0 = this.b0 / this.a0;[m
[31m-  this.b1a0 = this.b1 / this.a0;[m
[31m-  this.b2a0 = this.b2 / this.a0;[m
[31m-  this.a1a0 = this.a1 / this.a0;[m
[31m-  this.a2a0 = this.a2 / this.a0;[m
[31m-[m
[31m-  this.f0 = 3000;   // "wherever it's happenin', man."  Center Frequency or[m
[31m-                    // Corner Frequency, or shelf midpoint frequency, depending[m
[31m-                    // on which filter type.  The "significant frequency".[m
[31m-[m
[31m-  this.dBgain = 12; // used only for peaking and shelving filters[m
[31m-[m
[31m-  this.Q = 1;       // the EE kind of definition, except for peakingEQ in which A*Q is[m
[31m-                    // the classic EE Q.  That adjustment in definition was made so that[m
[31m-                    // a boost of N dB followed by a cut of N dB for identical Q and[m
[31m-                    // f0/Fs results in a precisely flat unity gain filter or "wire".[m
[31m-[m
[31m-  this.BW = -3;     // the bandwidth in octaves (between -3 dB frequencies for BPF[m
